    <ImplicitUsings>enable</ImplicitUsings>
    <UserSecretsId>a36684c2-a62a-4484-ab44-968b32f9f422</UserSecretsId>
    <DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
